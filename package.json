{
  "name": "yuelu-utils",
  "description": "使用 vite 构建的函数工具库",
  "keywords": [
    "vite",
    "function utils"
  ],
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:type": "vite build && tsc --project tsconfig.type.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "decimal.js": "^10.4.3",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "screenfull": "^6.0.2",
    "ua-parser-js": "^1.0.37",
    "url-parse": "^1.5.10",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/ua-parser-js": "^0.7.39",
    "@types/url-parse": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.6",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuelu-lan/yuelu-utils.git"
  },
  "author": "yuelu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yuelu-lan/yuelu-utils/issues"
  },
  "homepage": "https://github.com/yuelu-lan/yuelu-utils#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.cjs",
  "module": "./dist/my-lib.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs",
      "types": "./dist/types/main.d.ts"
    }
  }
}
